{
  "permissions": {
    "allow": [
      "Bash",
      "Fetch(*)",
      "Write(*)",
      "Edit(*)",
      "Read(*)",
      "Glob(*)",
      "Grep(*)",
      "mcp__plugin_playwright_playwright__browser_*",
      "WebFetch(*)"
    ],
    "deny": [
      "Read(.env*)",
      "Write(.env*)",
      "Edit(.env*)",
      "Bash(rm -rf /)",
      "Bash(sudo *)",
      "Bash(chmod 777 *)"
    ],
    "defaultMode": "acceptEdits"
  }
}
